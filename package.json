{
  "name": "luis-mocker",
  "version": "0.0.3",
  "description": "Http mocking for Luis.ai",
  "keywords": [
    "botbuilder",
    "luis",
    "luis.ai",
    "mock",
    "tdd",
    "bdd",
    "unit test",
    "e2e",
    "test"
  ],
  "main": "dist/src/index.js",
  "scripts": {
    "prepublishOnly": "npm run cleanBuild && node_modules/mocha/bin/_mocha ./dist/test --recursive",
