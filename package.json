    "nock": "^9.0.14"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microsoftly/luis-mocker.git"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.41",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.14",
    "@types/request": "^2.0.0",
    "@types/request-promise": "^4.1.36",
    "bluebird": "^3.5.0",
    "chai": "^4.1.0",
    "mocha": "^3.4.2",
